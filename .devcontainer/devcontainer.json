{
  "name": "judger",
  "dockerComposeFile": "docker-compose.yaml",
  "service": "code-goat-dev",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.defaultProfile.linux": "bash"
      },
      "extensions": [
        "EditorConfig.EditorConfig",
        "ms-python.python",
        "rust-lang.rust-analyzer",
        "GitHub.copilot",
        "golang.go"
      ]
    }
  },
  "remoteUser": "vscode"
}
